{
    "name": "OutcomesPerSequence",
    "path": "outcomes_per_seq.csv",
    "format": "csv",
    "mediatype": "text/csv",
    "profile": "tabular-data-resource",
    "$schema": "https://frictionlessdata.io/schemas/table-schema.json",
    "schema": {
        "fields": [
            {
                "name": "sequence_id",
                "type": "integer",
                "role": "key",
                "description": "Identifier for short sequence of temperature recording",
                "units": "NONE",
                "constraints": {
                    "required": "TRUE",
                    "minimum": "0",
                    "maximum": "INF"
                }
            },
            {
                "name": "did_overheat_binary_label",
                "type": "category_indicator",
                "role": "outcome",
                "description": "Indicator for whether sequence shows overheating (temp above freezing) or not",
                "units": "NONE",
                "constraints": {
                    "required": "TRUE",
                    "minimum": "0",
                    "maximum": "1"
                }
            }
        ],
        "missingValues": [
            "",
            "nan"
        ]
    }
}